                                  <TableColumn fx:id="postsCol" prefWidth="75.0" text="posts" />
                                  <TableColumn fx:id="followingCol" prefWidth="75.0" text="following" />
                                  <TableColumn fx:id="followersCol" prefWidth="75.0" text="followers" />
                                </columns>
